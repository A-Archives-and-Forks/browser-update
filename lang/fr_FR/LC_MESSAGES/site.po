#: stat.php:7
msgid "Statistics"
msgstr "Statistiques"

#: stat.php:17 index.php:33
#, php-format
msgid "<b>%s</b> sites are using this notification"
msgstr "<strong class=\"number\">%d</strong> sites utilisent Browser-Update.org."

#: stat.php:20 index.php:36
#, php-format
msgid "<b>%s</b> visitors have already updated their browser"
msgstr "<strong class=\"number\">%d</strong> visiteurs ont déjà mis leur navigateur à jour."

