msgid "Statistics"
msgstr "Estadísticas"

#: stat.php:17 index.php:33
#, php-format
msgid "<b>%s</b> sites are using this notification"
msgstr "<strong class=\"number\">%d</strong> sitios están usando el script de Browser-Update.org."

#: stat.php:20 index.php:36
#, php-format
msgid "<b>%s</b> visitors have already updated their browser"
msgstr "<strong class=\"number\">%d</strong> visitantes ya han actualizado su navegador."

#: stat.php:25
msgid "Browser updates"
msgstr ""

#: stat.php:207
