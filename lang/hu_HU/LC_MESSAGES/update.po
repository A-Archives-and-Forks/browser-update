msgid "Update your Browser"
msgstr "Frissítse böngészőjét"

#: header.php:29
msgid "An initiative by websites to inform users to update their web browser"
msgstr "Webdizájnerek kezdeményezése a felhasználók böngésző-frissítésről történő tájékoztatásáról"

#: header.php:31 update-browser.php:192
msgid "About the Project"
msgstr "A Projekt"

#: header.php:35
msgid "Statistics"
msgstr "Statisztikák"

#: header.php:36
