#: stat.php:7
msgid "Statistics"
msgstr "Statistika"

#: stat.php:17 index.php:33
#, php-format
msgid "<b>%s</b> sites are using this notification"
msgstr "<strong class=\"number\">%d</strong> web lapas izmanto Browser-Update.org skriptu."

#: stat.php:20 index.php:36
#, php-format
msgid "<b>%s</b> visitors have already updated their browser"
msgstr "<strong class=\"number\">%d</strong> apmeklētāji jau ir jatjauninājuši savu pārlūku."

#: stat.php:25
msgid "Browser updates"
msgstr ""

#: stat.php:207
msgid "From"
msgstr ""

#: stat.php:207
